        "assets": ["assets", "favicon.ico"],
        "index": "index.html",
        "main": "main.ts",
        "polyfills": "polyfills.ts",
        "test": "test.ts",
        "tsconfig": "tsconfig.app.json",
        "testTsconfig": "tsconfig.spec.json",
        "prefix": "app",
        "styles": ["../node_modules/primeng/resources/primeng.min.css", "./assets/mystyles/AdminLTE.css", "./assets/mystyles/daterangepicker.css", "./assets/mystyles/bootstrap-datepicker.css", "./assets/mystyles/bootstrap-timepicker.css", "../node_modules/bootstrap/dist/css/bootstrap.css", "../node_modules/font-awesome/css/font-awesome.min.css", "../node_modules/primeng/resources/themes/cupertino/theme.css", "../node_modules/angular5-toaster/dist/toaster.css", "../node_modules/moment/locale/es-do.js", "styles.css",
            "../node_modules/print-js/dist/print.css"],
        "scripts": ["../node_modules/jquery/dist/jquery.js", "./app/jsLib/jquery.dataTables.js", "../node_modules/bootstrap/dist/js/bootstrap.js", "./app/jsLib/adminlte.js", "./app/jsLib/bootstrap-timepicker.js", "../node_modules/chart.js/dist/Chart.js",
            "../node_modules/print-js/dist/print.js"],
